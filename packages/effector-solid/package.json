  "keywords": [
    "solid",
    "solid-js",
    "business",
    "logic",
    "data",
    "flow",
    "state management",
    "state manager",
    "algebraic effects",
    "model",
    "reactive",
    "state",
    "frp",
    "event",
    "effect",
    "functional",
    "store"
