    "effector-solid.cjs.js.map",
    "effector-solid.umd.js",
    "effector-solid.umd.js.map",
    "compat.js",
    "compat.js.map",
    "effector-solid.cjs.js.flow",
    "effector-solid.umd.js.flow",
    "compat.js.flow",
    "effector-solid.cjs.d.ts",
    "effector-solid.mjs.d.ts",
    "effector-solid.umd.d.ts",
    "compat.d.ts",
    "scope.js",
    "scope.js.map",
    "scope.mjs",
    "scope.mjs.map",
    "scope.d.ts"
