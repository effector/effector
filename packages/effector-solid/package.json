    "./scope.mjs": "./scope.mjs",
    "./scope": {
      "import": "./scope.mjs",
      "require": "./scope.js",
      "default": "./scope.mjs"
    },
    "./effector-solid.umd": "./effector-solid.umd.js"
  },
  "umd:main": "effector-solid.umd.js",
  "jsnext:main": "effector-solid.mjs",
